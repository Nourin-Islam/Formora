{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec \"tsx\" src/index.ts",
    "typecheck": "tsc --noEmit",
    "build": "tsup src/index.ts --splitting --format esm --dts --clean --outDir dist",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "test:dev": "NODE_OPTIONS='--loader ts-node/esm' vitest dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@clerk/backend": "^1.29.1",
    "@clerk/express": "^1.4.0",
    "@prisma/client": "^6.5.0",
    "@types/ws": "^8.18.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.5.0",
    "express-status-monitor": "^1.3.4",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.2.0",
    "imagekit": "^6.0.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.10.1",
    "svix": "^1.63.1",
    "ws": "^8.18.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "5.0.1",
    "@types/helmet": "^0.0.48",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "nodemon": "^3.1.9",
    "prisma": "^6.5.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.3.3",
    "vitest": "^3.1.1"
  },
  "prisma": {
    "seed": "npm run prisma:seed"
  }
}
